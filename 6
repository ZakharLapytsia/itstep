result = []

def divider(a, b):
    if not isinstance(a, (int, float)) or not isinstance(b, (int, float)):
        raise TypeError("Обидва аргументи мають бути числами.")
    if a < b:
        raise ValueError("Число 'a' має бути більше або дорівнювати 'b'.")
    if b > 100:
        raise IndexError("Число 'b' не повинно бути більшим за 100.")
    if b == 0:
        raise ZeroDivisionError("Ділення на нуль неможливе.")
    return a / b

data = {10: 2, 2: 5, "123": 4, 18: 0, (1, 2): 15, 8: 4}  

for key, value in data.items():
    try:
        res = divider(key, value)
        result.append(res)
    except (TypeError, ValueError, IndexError, ZeroDivisionError) as e:
        print(f"Помилка при обробці пари ({key}: {value}) -> {e}")

print("Результат:", result)
